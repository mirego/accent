{{#if this.model.loading}}
  <SkeletonUi::ProgressLine />
{{/if}}

{{#if this.showSkeleton}}
  <SkeletonUi::VersionsList />
{{else}}
  <VersionsList
    @permissions={{this.permissions}}
    @versions={{this.model.versions.entries}}
    @project={{this.model.project}}
  />

  {{#if (get this.permissions "create_version")}}
    <VersionsAddButton @project={{this.model.project}} />
  {{/if}}

  <ResourcePagination
    @meta={{this.model.versions.meta}}
    @onSelectPage={{fn this.selectPage}}
  />
  {{outlet}}
{{/if}}
