{{#if this.model.loading}}
  <SkeletonUi::ProgressLine />
{{/if}}

{{#if this.showLoading}}
  <LoadingContent @label={{t "pods.project.index.loading_content"}} />
{{else}}
  <DashboardRevisions
    @document={{this.document}}
    @project={{this.project}}
    @activities={{this.project.activities.entries}}
    @revisions={{this.revisions}}
    @permissions={{this.permissions}}
    @onCorrectAllConflicts={{fn this.correctAllConflicts}}
    @onUncorrectAllConflicts={{fn this.uncorrectAllConflicts}}
  />
{{/if}}
