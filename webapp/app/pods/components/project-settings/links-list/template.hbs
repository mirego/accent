  <div class="list">
    <LinkTo @route="logged-in.project.edit.collaborators" class="link">
      {{inline-svg "assets/users.svg" class="link-icon"}}
      {{t "components.project_settings.links_list.collaborators"}}
    </LinkTo>

    <LinkTo @route="logged-in.project.edit.badges" class="link">
      {{inline-svg "assets/badge.svg" class="link-icon"}}
