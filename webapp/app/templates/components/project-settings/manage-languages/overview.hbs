<ul local-class='list'>
  {{#each @revisions key='id' as |revision|}}
    <ProjectSettings::ManageLanguages::Overview::Item
      @master={{revision.isMaster}}
      @permissions={{@permissions}}
      @onPromoteMaster={{@onPromoteMaster}}
      @onDelete={{@onDelete}}
      @project={{@project}}
      @revision={{revision}}
    />
  {{/each}}
</ul>